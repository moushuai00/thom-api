#set($layout="velocity/layout/layout_nav.vm")
#set($pageTitle="创建接口")

<div class="panel panel-default">
    <div class="panel-heading">新增接口</div>
    <div class="panel-body">
        <form class="form-horizontal" role="form">
            <div class="form-group">
                <label for="actionName" class="col-sm-2 control-label">接口名称</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="actionName" placeholder="请输入您的接口名称">
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-2 control-label">请求类型</label>
                <div class="col-sm-10 form-inline">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox">POST
                        </label>
                    </div>
                    <div class="checkbox">
                        <label>
                            <input type="checkbox">GET
                        </label>
                    </div>
                    <div class="checkbox">
                        <label>
                            <input type="checkbox">POST
                        </label>
                    </div>
                    <div class="checkbox">
                        <label>
                            <input type="checkbox">DELETE
                        </label>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <label for="actionUrl" class="col-sm-2 control-label">接口链接</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="actionUrl" placeholder="请输入您的接口链接">
                </div>
            </div>
            <div class="form-group">
                <label for="actionDesc" class="col-sm-2 control-label">接口说明</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="actionDesc" placeholder="请输入您的接口说明">
                </div>
            </div>

            <div class="form-group">
                <label class="col-sm-2 control-label">返回格式</label>
                <div class="col-sm-10 form-inline">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox">对象{}
                        </label>
                    </div>
                    <div class="checkbox">
                        <label>
                            <input type="checkbox">数组[{}, {}]
                        </label>
                    </div>
                </div>
            </div>

            <div class="form-group">
                <label for="actionVersion" class="col-sm-2 control-label">版本</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="actionVersion" placeholder="请输入您的接口版本：V100">
                </div>
            </div>

            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10 text-right">
                    <button type="submit" class="btn btn-primary">确认新增</button>
                    <button type="submit" class="btn btn-default">重置</button>
                </div>
            </div>
        </form>
    </div>
</div>