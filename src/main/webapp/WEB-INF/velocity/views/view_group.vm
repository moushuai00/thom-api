        <table class="table table-hover table-striped">
            <thead>
            <tr>
                <th class="col-md-3 text-center"> 团队名称</th>
                <th class="col-md-3 text-center"> 创建人</th>
                <th class="col-md-3 text-center"> 人数</th>
                <th class="col-md-3 text-center"> 操作</th>
            </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
        <ul id="example"></ul>
    </div>
</form>